{
    "name": "formatter-todo-lang",
    "displayName": "formatter-todo-lang",
    "description": "Formats TODO files",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:todo-lang"
    ],
    "repository": {
        "url": "https://github.com/teraxas/vscode-todo-formatter",
        "type": "git"
    },
    "main": "./out/src/extension",
    "publisher": "teraxas",
    "author": "Karolis Joceviƒçius",
    "contributes": {
        "languages": [
            {
                "id": "todo-lang",
                "aliases": [
                    "Todo"
                ],
                "extensions": [
                    "todo"
                ]
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && tsc",
        "vsce": "vsce",
        "build": "tsc",
        "package": "npm run build && vsce package"
    },
    "devDependencies": {
        "@types/node": "^12.0.0",
        "typescript": "^3.9.3",
        "vsce": "^1.75.0",
        "vscode": "1.1.37"
    },
    "dependencies": {}
}
